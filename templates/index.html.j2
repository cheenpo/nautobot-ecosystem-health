<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
</head>

<body>

    <h1>{{ hello.world }}</h1>

    <table class="table">
        <thead>
            <tr>
                <th>App Name</th>
                <th>Upstream Testing Status</th>
                <th>Docs Status</th>
            </tr>
        </thead>
        <tbody class="table-striped">
            {% for app in apps %}
            <tr>
                <td>{{ app }}</td>
                <td>
                    <a href="https://github.com/nautobot/nautobot-app-{{ app }}/actions/workflows/upstream_testing.yml">
                        <img
                            src="https://github.com/nautobot/nautobot-app-{{ app }}/actions/workflows/upstream_testing.yml/badge.svg">
                    </a>
                </td>
                <td>
                    <a href="https://docs.nautobot.com/projects/{{ app }}/en/latest/"><img
                            src="https://readthedocs.org/projects/nautobot-plugin-{{ app }}/badge/"></a>
                </td>
            </tr>
            {% endfor %}
        </tbody>
    </table>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz"
        crossorigin="anonymous"></script>
</body>
</body>

</html>
